:root {
  --cur-pointer: pointer;
  --cur-default: default;
  --cur-notallowed: not-allowed;
  --cur-grab: grab;
  --cur-grabbing: grabbing;
  --cur-help: help;
  font-size: 1em;
}
@media screen and (min-width: 2000px) {
  :root {
    font-size: 1.5em;
  }
}
@media screen and (min-width: 3400px) {
  :root {
    font-size: 2.25em;
  }
}

html[custom-cursor] {
  --cur-pointer: url("/assets/img/cursors/cursor-pointer.cur"), pointer;
  --cur-default: url("/assets/img/cursors/cursor-default.cur"), default;
  --cur-notallowed: url("/assets/img/cursors/cursor-notallowed.cur"), not-allowed;
  --cur-grab: url("/assets/img/cursors/cursor-grab.cur"), grab;
  --cur-grabbing: url("/assets/img/cursors/cursor-grab.cur"), grabbing;
  --cur-help: url("/assets/img/cursors/cursor-help.cur"), help;
}

html {
  cursor: var(--cur-default);
  background: #000;
  color: #fff;
  font-family: monospace;
  font-size: 1em;
  font-weight: 500;
  max-height: 100%;
  height: 100%;
}

body {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  gap: 0.25em;
  padding: 0.5em;
  box-sizing: border-box;
}

main {
  box-sizing: border-box;
  height: 100%;
  overflow-y: auto;
  min-height: 20em;
  margin: 0.25em 0;
}

.h-100 {
  height: 100%;
}

pre {
  margin: 0;
}

iframe {
  border: 0;
}

.no-js-warning {
  display: block;
  background: #ffbb00;
  color: #000000;
  padding: 0.25em;
  border-radius: 0.5em;
  text-align: center;
  font-weight: 600;
  font-size: 1.25em;
}

.infobar {
  color: #cacaca;
}

::-webkit-scrollbar {
  width: 3px;
  height: 3px;
}

::-webkit-scrollbar-track-piece {
  background-color: RGBA(0, 0, 0, 0);
}

::-webkit-scrollbar-thumb:vertical {
  height: 3px;
  background-color: #2c2c2c;
}

* {
  scrollbar-width: thin;
  scrollbar-color: #2c2c2c RGBA(0, 0, 0, 0);
}

.container {
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  gap: 0.25em;
  max-width: 860px;
  width: 100%;
  height: 100%;
}
@media screen and (min-width: 2000px) {
  .container {
    max-width: 1260px;
  }
}
@media screen and (min-width: 3400px) {
  .container {
    max-width: 2000px;
  }
}

h6:not([nobold]),
.h6:not([nobold]) {
  font-weight: 600;
}
h6,
.h6 {
  font-size: 1.1em;
}

h5:not([nobold]),
.h5:not([nobold]) {
  font-weight: 600;
}
h5,
.h5 {
  font-size: 1.2em;
}

h4:not([nobold]),
.h4:not([nobold]) {
  font-weight: 600;
}
h4,
.h4 {
  font-size: 1.25em;
}

h3:not([nobold]),
.h3:not([nobold]) {
  font-weight: 600;
}
h3,
.h3 {
  font-size: 1.5em;
}

h2:not([nobold]),
.h2:not([nobold]) {
  font-weight: 600;
}
h2,
.h2 {
  font-size: 2em;
}

h1:not([nobold]),
.h1:not([nobold]) {
  font-weight: 600;
}
h1,
.h1 {
  font-size: 2.5em;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
}
h1:not([nobold]),
h2:not([nobold]),
h3:not([nobold]),
h4:not([nobold]),
h5:not([nobold]),
h6:not([nobold]) {
  font-weight: 600;
}

ul, ol {
  padding-left: 2.5em;
  margin: 0.5em 0;
}

button, input {
  all: unset;
}

input[type=checkbox] {
  display: inline-block;
  all: unset;
  padding: 0;
  margin: 0;
  max-width: 1em;
  max-height: 1em;
  min-width: 1em;
  min-height: 1em;
  border: 0.1em solid #363636;
  background: rgba(0, 0, 0, 0.8);
  border-radius: 0.25em;
  font-family: "bootstrap-icons";
  cursor: var(--cur-pointer);
}
input[type=checkbox]::before {
  color: rgba(0, 0, 0, 0);
  line-height: 0.9em;
  font-size: 1em;
  content: "\f633";
}
input[type=checkbox]:checked::before {
  color: rgb(255, 255, 255);
  content: "\f633";
}

.button {
  all: unset;
  background: #141414;
  border: #363636 solid 0.1em;
  border-radius: 0.5em;
  box-sizing: border-box;
  padding: 0.4em;
  cursor: var(--cur-pointer);
  user-select: none;
  transition: background 0.15s ease;
}
.button:hover {
  transition: background 0.1s ease;
  background: #222222;
}
.button:active {
  transition: background 0.1s ease;
  background: #000000;
}
.button[disabled] {
  background: #000;
  cursor: var(--cur-notallowed);
}

.boop-button {
  display: flex;
  flex-direction: row;
  gap: 0.5em;
  align-items: center;
  font-size: 1.05em;
  user-select: none;
}
.boop-button .column {
  display: flex;
  flex-direction: column;
  gap: 0.25em;
}

.input {
  all: unset;
  color: white;
  background: #000;
  border-radius: 0.5em;
  padding: 0.4em;
  box-sizing: border-box;
  border: #363636 solid 0.1em;
}
.input[error] {
  border: #ab3939 solid 0.1em;
}

input[type=range] {
  all: unset;
}

/* chrome */
input[type=range]::-webkit-slider-thumb {
  -webkit-appearance: none;
  padding-bottom: 0.25em;
  width: 20px;
  height: 20px;
  margin: -5px 0;
  background-color: #6a2ecc;
  border-radius: 100%;
  cursor: var(--cur-pointer);
}

input[type=range]::-webkit-slider-runnable-track {
  width: 100%;
  height: 10px;
  height: 10px;
  cursor: var(--cur-pointer);
  border: 1px solid #363636;
  background: linear-gradient(90deg, #000, #8438ff);
  box-sizing: border-box;
  border-radius: 0.5em;
}

input[type=range]:focus::-webkit-slider-thumb {
  cursor: var(--cur-grabbing);
  box-shadow: 0 0 0 2px #4a208d;
}

input[type=range]::-webkit-sider {
  height: 10px;
  background: #8438ff;
  border-radius: 0.5em;
}

input[type=range][small]::-webkit-slider-thumb {
  margin: -4px 0;
  width: 15px;
  height: 15px;
}

input[type=range][small]::-webkit-slider-runnable-track, input[type=range]::-webkit-sider {
  height: 7px;
}

/* firefox */
input[type=range]::-moz-range-thumb {
  width: 20px;
  height: 20px;
  background-color: #6a2ecc;
  border-radius: 100%;
  border: none;
  cursor: var(--cur-pointer);
}

input[type=range]::-moz-range-track {
  width: 100%;
  height: 10px;
  cursor: var(--cur-pointer);
  border: 1px solid #363636;
  background: #000;
  box-sizing: border-box;
  border-radius: 0.5em;
}

input[type=range]:focus {
  outline: none;
}

input[type=range]:focus::-moz-range-thumb {
  cursor: var(--cur-grabbing);
  box-shadow: 0 0 0 2px #4a208d;
}

input[type=range]::-moz-range-progress {
  height: 10px;
  background-color: #8438ff;
  border-bottom-left-radius: 0.5em;
  border-top-left-radius: 0.5em;
}

input[type=range][small]::-moz-range-thumb {
  width: 15px;
  height: 15px;
}

input[type=range][small]::-moz-range-track, input[type=range][small]::-moz-range-progress {
  height: 7px;
}

.user-bar {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  gap: 0.5em;
  padding-bottom: 0.25em;
  border-bottom: #363636 solid 0.1em;
}
.user-bar .online {
  order: 1;
  cursor: var(--cur-help);
  display: flex;
  flex-direction: row;
  align-items: center;
  gap: 0.3em;
  padding: 0.2em 0;
}
.user-bar .online .dot {
  cursor: var(--cur-help);
  color: #3cf024;
  text-shadow: 0 0 6px #9af48e;
  font-size: 0.7em;
}
.user-bar .online .dot[updated] {
  animation: online-dot-blink 0.9s ease 0s 1 forwards;
}
@keyframes online-dot-blink {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
.user-bar .online .text {
  cursor: var(--cur-help);
  color: #ace973;
  text-shadow: 0 0 7px #ace973;
}
.user-bar .items {
  order: 3;
}
.user-bar .items.items-main {
  flex-wrap: wrap;
}
@media screen and (min-width: 1px) {
  .user-bar .items.items-main {
    flex-basis: 100%;
  }
}
@media screen and (min-width: 568px) {
  .user-bar .items.items-main {
    flex-basis: auto;
  }
}
.user-bar .items {
  display: flex;
  flex-direction: row;
  gap: 0.33em;
  padding: 0.2em 0;
}
.user-bar .items.loggedout {
  margin-left: auto;
}
@media screen and (min-width: 1px) {
  .user-bar .items.loggedout {
    order: 2;
  }
}
@media screen and (min-width: 568px) {
  .user-bar .items.loggedout {
    margin-left: auto;
    order: 4;
  }
}
.user-bar .items .item {
  display: flex;
  flex-direction: row;
  gap: 0.4em;
  align-items: center;
  cursor: var(--cur-pointer);
  transition: font-size 0.4s ease;
  text-shadow: 0 0 7px #ace973;
  color: #ace973;
  font-size: 1em;
}
.user-bar .items .item[active] {
  cursor: var(--cur-default);
  color: #7e8e6f;
  font-weight: 600;
  text-shadow: 0 0 14px #ace973;
}
.user-bar .items .item:hover {
  font-weight: 600;
}
.user-bar .username-container {
  margin-left: auto;
}
.user-bar .username-container .username {
  display: flex;
  flex-direction: row;
  gap: 0.25em;
  align-items: center;
  cursor: var(--cur-pointer);
}
.user-bar .username-container .username .text {
  font-size: 1em;
}
.user-bar .username-container .username .icon {
  font-size: 0.95em;
}
.user-bar .username-container .dropdown {
  max-width: 15em;
  width: 100%;
  border: #363636 solid 0.1em;
  box-sizing: border-box;
  border-radius: 0.5em;
  background: #1a1a1a;
  padding: 0.33em;
  margin-top: 0.75em;
  z-index: 5;
}
.user-bar .username-container .dropdown .button {
  width: 100%;
}
.user-bar .username-container .dropdown .logout-btn {
  border: #ee5252 solid 0.1em;
  border-radius: 0.5em;
  background: #a12626;
  text-align: center;
}
.user-bar .username-container .dropdown .logout-btn:hover {
  background: #d53737;
}
.user-bar .username-container .dropdown .logout-btn:active {
  background: #541515;
}
@media screen and (min-width: 1px) {
  .user-bar .username-container {
    order: 2;
  }
}
@media screen and (min-width: 568px) {
  .user-bar .username-container {
    margin-left: auto;
    order: 4;
  }
}

.user-profile {
  display: flex;
  flex-direction: column;
  gap: 0.5em;
}
.user-profile .posts-n-info-row {
  display: flex;
  flex-direction: row;
  gap: 0.25em;
}
.user-profile .posts-n-info-row .info-side {
  flex-basis: 15em;
  border-radius: 0.5em;
  border: #363636 solid 0.1em;
  padding: 0.25em;
}
.user-profile .posts-n-info-row .user-posts {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  gap: 0.25em;
}
.user-profile .user-frontface {
  position: relative;
  z-index: 0;
  border: #363636 0.1em solid;
  border-radius: 0.5em;
  box-sizing: border-box;
  overflow: hidden;
  height: 14em;
}
.user-profile .user-frontface .user-banner {
  z-index: 1;
  position: absolute;
  background-color: #000;
  background-image: url("/assets/img/banner.png");
  background-size: cover;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  filter: blur(0.4em) brightness(60%);
  width: 100%;
  height: 100%;
}
.user-profile .user-frontface .user-row {
  position: absolute;
  z-index: 2;
  display: flex;
  flex-direction: row;
  gap: 0.5em;
  padding: 0.5em;
  box-sizing: border-box;
  height: 100%;
}
.user-profile .user-frontface .user-row .user-info {
  display: flex;
  gap: 0.25em;
  flex-direction: column;
  border-radius: 0.5em;
  box-sizing: border-box;
  flex: 1;
}
.user-profile .user-frontface .user-row .user-info .username {
  font-weight: 500;
  font-size: 1.75em;
}
.user-profile .user-frontface .user-row .user-info .user-row-buttons {
  display: flex;
  flex-direction: row;
  gap: 0.25em;
  margin-top: auto;
}
.user-profile .user-frontface .user-row .user-picture {
  background-image: url("/assets/img/bepisdrink-transparent.png");
  background-size: 100% 100%;
  aspect-ratio: 1/1;
  border-radius: 0.5em;
  border: #363636 solid 0.1em;
  box-sizing: border-box;
  background-color: rgba(0, 0, 0, 0.7);
  height: 100%;
}

.navbar {
  display: flex;
  flex-direction: column;
  width: 100%;
  background: linear-gradient(90deg, #43217a 0%, #793adf 25%, #43217a 50%, #793adf 75%, #43217a 100%);
  animation: gradient-pos 12s linear 0s infinite forwards;
  background-size: 200% 100%;
  border-radius: 0.5em;
  box-sizing: border-box;
  border: #363636 solid 0.1em;
}

@keyframes gradient-pos {
  0% {
    background-position: 0% 0;
  }
  100% {
    background-position: 200% 0;
  }
}
.navbar-branding {
  position: relative;
  background: #000;
  background-image: url("/assets/img/banner.png");
  background-repeat: no-repeat;
  background-size: cover;
  background-position: 50%;
  padding: 0.5em;
  flex-grow: 1;
  border-top-right-radius: 0.5em;
  border-top-left-radius: 0.5em;
  padding-top: 7.5em;
  box-sizing: border-box;
}
.navbar-branding .credit {
  position: absolute;
  color: #ededed;
  top: 0.5em;
  right: 0.5em;
  text-shadow: 0 0 0 transparent, -1px -1px 0 #000, 0px -1px 0 #000, 1px -1px 0 #000, -1px 0px 0 #000, 0px 0px 0 #000, 1px 0px 0 #000, -1px 1px 0 #000, 0px 1px 0 #000, 1px 1px 0 #000;
  font-size: 0.85em;
}
.navbar-branding .version {
  position: absolute;
  color: #ededed;
  top: 0.5em;
  left: 0.5em;
  text-shadow: 0 0 0 transparent, -1px -1px 0 #000, 0px -1px 0 #000, 1px -1px 0 #000, -1px 0px 0 #000, 0px 0px 0 #000, 1px 0px 0 #000, -1px 1px 0 #000, 0px 1px 0 #000, 1px 1px 0 #000;
  font-size: 0.85em;
}
.navbar-branding .local-settings {
  position: absolute;
  color: #ededed;
  bottom: 0.5em;
  right: 0.5em;
  text-shadow: 0 0 0 transparent, -1px -1px 0 #000, 0px -1px 0 #000, 1px -1px 0 #000, -1px 0px 0 #000, 0px 0px 0 #000, 1px 0px 0 #000, -1px 1px 0 #000, 0px 1px 0 #000, 1px 1px 0 #000;
  font-size: 1.25em;
}
.navbar-branding .brand {
  text-shadow: 0 0 0 transparent, -1px -1px 0 #000, 0px -1px 0 #000, 1px -1px 0 #000, -1px 0px 0 #000, 0px 0px 0 #000, 1px 0px 0 #000, -1px 1px 0 #000, 0px 1px 0 #000, 1px 1px 0 #000, -2px -2px 0 #000, -1px -2px 0 #000, 0px -2px 0 #000, 1px -2px 0 #000, 2px -2px 0 #000, -2px -1px 0 #000, -1px -1px 0 #000, 0px -1px 0 #000, 1px -1px 0 #000, 2px -1px 0 #000, -2px 0px 0 #000, -1px 0px 0 #000, 0px 0px 0 #000, 1px 0px 0 #000, 2px 0px 0 #000, -2px 1px 0 #000, -1px 1px 0 #000, 0px 1px 0 #000, 1px 1px 0 #000, 2px 1px 0 #000, -2px 2px 0 #000, -1px 2px 0 #000, 0px 2px 0 #000, 1px 2px 0 #000, 2px 2px 0 #000;
