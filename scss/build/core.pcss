}
.index-page .cat-ascii {
  margin-top: 1em;
  display: flex;
  flex-direction: column;
}
.index-page .cat-ascii * {
  font-family: Arial, Helvetica, sans-serif !important;
  font-weight: 500;
}

.drawing-ide {
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 100%;
  gap: 0.25em;
}
.drawing-ide .drawing-canvas {
  position: relative;
  display: flex;
  flex-direction: row;
  flex: 1;
  height: 100%;
  border-radius: 0.5em;
  box-sizing: border-box;
  border: #363636 solid 0.1em;
  overflow: hidden;
}
.drawing-ide .drawing-canvas .color-bar {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 0.25em;
  overflow-y: auto;
  box-sizing: border-box;
  min-width: 3.5em;
  padding: 0.5em;
}
.drawing-ide .drawing-canvas .color-bar > .color {
  border-radius: 0.5em;
  aspect-ratio: 1/1;
  width: 100%;
  box-sizing: border-box;
  border: #363636 solid 0.1em;
}
.drawing-ide .drawing-canvas .color-bar > .color.click {
  cursor: var(--cur-pointer);
}
.drawing-ide .drawing-canvas .color-bar > .color.active {
  border: #804eff solid 0.1em;
}
.drawing-ide .drawing-canvas .color-bar .flip-me-bitch {
  writing-mode: sideways-lr;
  margin: auto;
}
.drawing-ide .drawing-canvas .color-bar > .divider {
  width: 100%;
  box-sizing: border-box;
  border-bottom: #363636 solid 0.1em;
  margin: 0.25em 0;
}
.drawing-ide .drawing-canvas .color-bar > .divider.bottom {
  margin-top: auto;
}
.drawing-ide .drawing-canvas .color-bar .control {
  position: relative;
