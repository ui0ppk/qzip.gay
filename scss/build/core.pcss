.classic-spinner {
  width: 15px;
  aspect-ratio: 1;
  border-radius: 50%;
  animation: l5 1s infinite linear backwards;
}

@keyframes l5 {
  0% {
    box-shadow: 20px 0 rgba(255, 255, 255, 0.2666666667), -20px 0 rgba(255, 255, 255, 0.1333333333);
    background: #fff;
  }
  20% {
    box-shadow: 20px 0 rgba(255, 255, 255, 0.2), -20px 0 #fff;
    background: #fff;
  }
  40% {
    box-shadow: 20px 0 rgba(255, 255, 255, 0.1333333333), -20px 0 #fff;
    background: rgba(255, 255, 255, 0.2666666667);
  }
  60% {
    box-shadow: 20px 0 #fff, -20px 0 #fff;
    background: rgba(255, 255, 255, 0.2);
  }
  80% {
    box-shadow: 20px 0 #fff, -20px 0 rgba(255, 255, 255, 0.2666666667);
    background: rgba(255, 255, 255, 0.1333333333);
  }
  100% {
    box-shadow: 20px 0 #fff, -20px 0 rgba(255, 255, 255, 0.2);
    background: #fff;
  }
}
.popups {
  position: fixed;
  z-index: 20;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100vw;
  height: 100vh;
  justify-content: center;
  overflow: auto;
  background: rgba(0, 0, 0, 0.25);
  backdrop-filter: blur(0.2em);
  display: flex;
  opacity: 1;
  visibility: visible;
  transition: visibility 0.2s, opacity 0.2s ease;
}
.popups[hidden] {
  transition: visibility 0.2s, opacity 0.2s ease;
  opacity: 0;
  visibility: hidden;
}
.popups .popups-items {
  padding: 3em 0.5em;
  box-sizing: border-box;
  position: absolute;
  overflow: auto;
  display: flex;
  flex-direction: column;
  gap: 0.5em;
  max-width: 36em;
  width: 100%;
}
.popups .popups-items .popup {
  display: flex;
  flex-direction: column;
  border: #363636 solid 0.1em;
  box-sizing: border-box;
  border-radius: 0.5em;
  width: 100%;
  overflow: hidden;
}
.popups .popups-items .popup .bar {
  background: #151515;
  border-bottom: #363636 solid 0.1em;
  display: flex;
  flex-direction: row;
  align-items: center;
  gap: 0.25em;
  padding: 0.25em;
}
.popups .popups-items .popup .bar .title {
  font-size: 1.2em;
  padding: 0.1em;
  overflow: hidden;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  line-clamp: 3;
}
.popups .popups-items .popup .bar .btns {
  display: flex;
  flex-direction: row;
  gap: 0.25em;
  margin-left: auto;
}
.popups .popups-items .popup .bar .btns > * {
  padding: 0.25em;
}
.popups .popups-items .popup .bar .btns > *.btn-close {
  border: #ab3939 solid 0.1em;
  background: #551f1f;
}
.popups .popups-items .popup .body {
  background: rgba(0, 0, 0, 0.75);
  padding: 0.35em;
}
.popups .popups-items .popup .buttons {
  display: flex;
  flex-direction: row;
  gap: 0.25em;
  background: rgba(0, 0, 0, 0.75);
  border-top: #262626 solid 0.1em;
  padding: 0.35em;
}
.popups .close-all {
  position: absolute;
  right: 0;
  top: 0;
  font-size: 1em;
  margin: 0.5em;
}

[x-cloak] {
  display: none !important;
}

