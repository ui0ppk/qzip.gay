.navbar-items {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  gap: 0.25em;
  padding: 0.2em 0;
  min-height: 2em;
  border-top: 0.1em solid #363636;
}
.navbar-items .item {
  display: flex;
  flex-direction: row;
  gap: 0.4em;
  align-items: center;
  cursor: var(--cur-pointer);
  transition: font-size 0.4s ease;
  padding: 0 0.75em;
