  flex-basis: 40em;
}
.index-page .silly-container {
  display: flex;
  flex-direction: column;
  gap: 0.25em;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  padding: 2em 4em;
  margin: 0 auto;
}
.index-page .silly-container .silly-text {
